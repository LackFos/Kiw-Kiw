{
  "name": "Mimic",
  "permissions": ["history", "storage", "tabs"],
  "host_permissions": ["<all_urls>"],
  "description": "Laravel Backpack extension to help you easily impersonate other users just one click",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "./popup/popup.html",
    "default_icon": "./assets/images/logo.png"
  },
  "icons": {
    "16": "./assets/images/logo_16.png",
    "48": "./assets/images/logo_48.png",
    "128": "./assets/images/logo_128.png"
  },
  "background": {
    "service_worker": "./background.js",
    "type": "module"
  }
}
